{
  "name": "fetch-figma-frame",
  "displayName": "fetch-figma-frame",
  "description": "It fetches a frame from figma",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:plaintext",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fetch-figma-frame.fetchFrame",
        "title": "Fetch figma frame in react"
      },
      {
        "command": "fetch-figma-frame.fetchFrameData",
        "title": "Fetch figma raw json data"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.108.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.21.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@adaline/anthropic": "^0.23.0",
    "@adaline/gateway": "^0.28.0",
    "@adaline/openai": "^0.26.0",
    "@adaline/types": "^0.18.0",
    "axios": "^1.8.3",
    "dotenv": "^16.4.7",
    "open": "^10.1.0",
    "proxy-agent": "^6.5.0"
  }
}
